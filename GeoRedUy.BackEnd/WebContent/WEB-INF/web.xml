<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>GeoRedUy.BackEnd</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>Layout</servlet-name>
    <servlet-class>georeduy.backend.controllers.Layout</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginLayout</servlet-name>
    <servlet-class>georeduy.backend.controllers.LoginLayout</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>georeduy.backend.controllers.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Home</servlet-name>
    <servlet-class>georeduy.backend.controllers.HomeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Test</servlet-name>
    <servlet-class>georeduy.backend.controllers.TestServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Event</servlet-name>
    <servlet-class>georeduy.backend.controllers.EventServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Site</servlet-name>
    <servlet-class>georeduy.backend.controllers.SiteServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Tag</servlet-name>
    <servlet-class>georeduy.backend.controllers.TagsServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Retail</servlet-name>
    <servlet-class>georeduy.backend.controllers.RetailServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Home</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Layout</servlet-name>
    <url-pattern>/layout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginLayout</servlet-name>
    <url-pattern>/login_layout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Test</servlet-name>
    <url-pattern>/test</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Site</servlet-name>
    <url-pattern>/site</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>Event</servlet-name>
    <url-pattern>/event</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Tag</servlet-name>
    <url-pattern>/tag</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Retail</servlet-name>
    <url-pattern>/retail</url-pattern>
  </servlet-mapping>
  
	<filter>
		<filter-name>LayoutFilter</filter-name>
		<filter-class>georeduy.backend.util.LayoutFilter</filter-class>
	</filter>

	<filter-mapping>
		<filter-name>LayoutFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
</web-app>